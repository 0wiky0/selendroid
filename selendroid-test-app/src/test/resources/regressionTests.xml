<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Selendroid regression test suite" parallel="false"
	verbose="1">
	<test name="selendroid element finding tests">
		<classes>
			<class
				name="io.selendroid.nativetests.NativeChildElementFindingTest" />
			<class name="io.selendroid.nativetests.NativeElementFindingTest" />
			<class
				name="io.selendroid.webviewdrivertests.WebElementFindingTest" />
			<class
				name="io.selendroid.webviewdrivertests.WebChildElementFindingTest" />
		</classes>
	</test>
	<test name="selendroid element interaction tests">
		<classes>
			<class
				name="io.selendroid.nativetests.NativeElementInteractionTests" />
			<class
				name="io.selendroid.webviewdrivertests.WebElementInteractionTests" />
		</classes>
	</test>


	<listeners>
		<listener class-name="io.selendroid.support.LargeTestFeedbackListener"></listener>
	</listeners>
</suite>